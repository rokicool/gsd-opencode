{
  "name": "TACHES 1.20.5 to OpenCode Translation",
  "version": "1.20.5",
  "description": "Converts Claude Code syntax to OpenCode syntax for GSD prompts",
  "patterns": ["gsd-opencode/**/*"],
  "include": [],
  "exclude": ["node_modules/**", ".git/**", ".translate-backups/**", "*.json"],
  "maxFileSize": 10485760,
  "rules": [
    {
      "name": "Command name: gsd:plan-phase -> gsd-plan-phase",
      "pattern": "gsd:plan-phase",
      "replacement": "gsd-plan-phase"
    },
    {
