    },
    {
      "name": "Command name: gsd:resume-work -> gsd-resume-work",
      "pattern": "gsd:resume-work",
      "replacement": "gsd-resume-work"
    },
    {
      "name": "Command name: gsd:quick -> gsd-quick",
      "pattern": "gsd:quick",
      "replacement": "gsd-quick"
    },
    {
      "name": "Command name: gsd:whats-new -> gsd-whats-new",
      "pattern": "gsd:whats-new",
      "replacement": "gsd-whats-new"
    },
    {
      "name": "Command name: gsd:help -> gsd-help",
