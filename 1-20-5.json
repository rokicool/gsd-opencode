    {
      "name": "Command name: gsd:map-codebase -> gsd-map-codebase",
      "pattern": "gsd:map-codebase",
      "replacement": "gsd-map-codebase"
    },
    {
      "name": "Command name: gsd:verify-work -> gsd-verify-work",
      "pattern": "gsd:verify-work",
      "replacement": "gsd-verify-work"
    },
    {
      "name": "Command name: gsd:progress -> gsd-progress",
      "pattern": "gsd:progress",
      "replacement": "gsd-progress"
    },
    {
      "name": "Command name: gsd:settings -> gsd-settings",
